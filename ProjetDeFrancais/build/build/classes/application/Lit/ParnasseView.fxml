<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="450.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.PopupWindowController">
   <children>
      <Label layoutX="109.0" layoutY="39.0" text="Parnasse : Littérature" AnchorPane.topAnchor="39.0">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Label layoutX="111.0" layoutY="78.0" text="La coupe de Leconte de Lisle" AnchorPane.topAnchor="78.0" />
      <VBox layoutX="63.0" layoutY="112.0" prefHeight="264.0" prefWidth="275.0" spacing="2.0">
         <children>
            <Label text="Prends ce bloc d'argent, adroit ciseleur. " />
            <Label text="N'en fais point surtout d'arme belliqueuse," />
            <Label text="Mais bien une coupe élargie et creuse " />
            <Label text="Où le vin ruisselle et semble meilleur. " />
            <Label text="Ne grave à l'entour Bouvier ni Pléiades, " />
            <Label text="Mais le choeur joyeux des belles Mainades, " />
            <Label text="Et l'or des raisins chers à l'oeil ravi, " />
            <Label text="Et la verte vigne, et la cuve ronde " />
            <Label text="Où les vendangeurs foulent à l'envi, " />
            <Label text="De leurs pieds pourprés, la grappe féconde. " />
            <Label text="Que j'y voie encore Evoé vainqueur, " />
            <Label text="Aphrodite, Éros et les Hyménées, " />
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Et sous les grands bois les vierges menées " />
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="La verveine au front et l'amour au coeur !" />
         </children>
      </VBox>
      <Button layoutX="169.0" layoutY="390.0" mnemonicParsing="false" onAction="#handleClose" text="Fermer" AnchorPane.bottomAnchor="33.0" />
   </children>
</AnchorPane>
