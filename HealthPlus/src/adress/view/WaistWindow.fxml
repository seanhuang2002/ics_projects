<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="300.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="adress.view.WaistWindowController">
   <children>
      <Label layoutX="161.0" layoutY="33.0" text="Waist Circumference" AnchorPane.leftAnchor="161.0" AnchorPane.rightAnchor="160.0">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Text layoutX="73.0" layoutY="90.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Waist circumference is an important indicator of body fat levels. It is mesured at the waist, not hips. A larger than normal waist circumference indicates higher than normal body fat levels, indicating poor health. The level of health depends on sex. Enter your waist circumference to find your health levels. " wrappingWidth="341.564453125" />
      <RadioButton fx:id="maleButton" layoutX="103.0" layoutY="185.0" mnemonicParsing="false" text="Male">
         <toggleGroup>
            <ToggleGroup fx:id="a" />
         </toggleGroup></RadioButton>
      <RadioButton fx:id="femaleButton" layoutX="103.0" layoutY="212.0" mnemonicParsing="false" text="Female" toggleGroup="$a" />
      <TextField fx:id="field" layoutX="250.0" layoutY="190.0" AnchorPane.leftAnchor="250.0" />
      <Button layoutX="251.0" layoutY="244.0" mnemonicParsing="false" onAction="#enter" text="Enter" />
      <Button layoutX="197.0" layoutY="244.0" mnemonicParsing="false" onAction="#goBack" text="Close" />
   </children>
</AnchorPane>
